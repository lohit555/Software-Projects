#!/bin/bash
#
# A simple framework for testing the Wordex (bctest) program
#
# Returns the number of failed test cases.
#
# Format of a test:
#     test 'command' expected_return_value 'stdin text' 'expected stdout' 'expected stderr'
#

# GLOBALS: tc = test case number, fails = number of failed cases
declare -i tc=0
declare -i fails=0

############################################
# Run a single test.
# Runs the command ONCE to check
# return value, stdout, stderr.
############################################
test() {
    tc=tc+1

    local COMMAND=$1
    local RETURN=$2
    local STDIN=$3
    local STDOUT=$4 
    local STDERR=$5

    # Instead of running the testscript multiple times running it once to avoid contaminating data
    local OUTPUT_FILE=$(mktemp)
    local ERROR_FILE=$(mktemp)

    $COMMAND <<< "$STDIN" >$OUTPUT_FILE 2>$ERROR_FILE
    local A_RETURN=$?

    local A_STDOUT=$(cat $OUTPUT_FILE)
    local A_STDERR=$(cat $ERROR_FILE)

    rm -f $OUTPUT_FILE $ERROR_FILE

    # If timeout (exit code 124), treat as failure
    if [[ "$A_RETURN" == "124" ]]; then
        echo "Test $tc Failed (Timeout)"
        echo "   $COMMAND"
        fails=$fails+1
        return 1
    fi

    if [[ "$A_RETURN" != "$RETURN" ]]; then
        echo "Test $tc Failed"
        echo "   $COMMAND"
        echo "   Expected Return: $RETURN"
        echo "   Actual Return: $A_RETURN"
        fails=$fails+1
        return 1
    fi

    if [[ "$STDOUT" != "$A_STDOUT" ]]; then
        echo "Test $tc Failed"
        echo "   $COMMAND"
        echo "   Expected STDOUT:"
        echo "$STDOUT"
        echo "   Actual STDOUT:"
        echo "$A_STDOUT"
        fails=$fails+1
        return 2
    fi

    if [[ "$STDERR" != "$A_STDERR" ]]; then
        echo "Test $tc Failed"
        echo "   $COMMAND"
        echo "   Expected STDERR:"
        echo "$STDERR"
        echo "   Actual STDERR:"
        echo "$A_STDERR"
        fails=$fails+1
        return 3
    fi
    
    # SUCCESS
    echo "Test $tc Passed"
    return 0
}

##########################################
# TEST CASES FOR WORDEX
##########################################

# Test 1 
test ./bctest 1 \
"3" \
$'
